{
  "name": "wpstorm-messenger",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preview": "npm-run-all --parallel sync wpstart tailwindwatch",
    "sync": "browser-sync start -p wpstormchat.local --files build/*.js build/*.css",
    "build": "npm-run-all --sequential buildwp tailwindbuild",
    "buildwp": "wp-scripts build src/index.js",
    "wpstart": "wp-scripts start src/index.js",
    "start": "npm-run-all --parallel wpstart tailwindwatch",
    "tailwindbuild": "tailwindcss -i ./src/index.css -o ./build/index.css --postcss --minify",
    "tailwindwatch": "tailwindcss -i ./src/index.css -o ./build/index.css --watch --postcss --minify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emoji-mart/data": "^1.1.2",
    "@emoji-mart/react": "^1.1.1",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@wordpress/element": "^5.18.0",
    "@wordpress/i18n": "^4.41.0",
    "@wordpress/scripts": "^26.12.0",
    "autoprefixer": "^10.4.15",
    "axios": "^1.5.0",
    "browser-sync": "^2.29.3",
    "emoji-mart": "^5.5.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.29",
    "postcss-nested": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-router-dom": "^6.15.0",
    "tailwindcss": "^3.3.3"
  }
}
