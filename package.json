{
  "name": "wpstorm-messenger",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preview": "npm-run-all --parallel sync startwp tailwindwatch",
    "adminpreview": "npm-run-all --parallel adminsync adminwpstart admintailwindwatch",
    "sync": "browser-sync start -p localhost/wpstorm-chat --files build/*.js build/*.css",
    "build": "npm-run-all --sequential buildwp tailwindbuild",
    "start": "npm-run-all --parallel startwp tailwindwatch",
    "buildwp": "wp-scripts build src/index.js --output-path=build/chat-widget",
    "startwp": "wp-scripts start src/index.js --output-path=build/chat-widget",
    "tailwindbuild": "tailwindcss -i ./src/index.css -o ./build/chat-widget/index.css --postcss --minify",
    "tailwindwatch": "tailwindcss -i ./src/index.css -o ./build/chat-widget/index.css --watch --postcss --minify",
    "adminsync": "browser-sync start -p localhost/wpstorm-chat --files build/*.js build/*.css",
    "adminbuild": "npm-run-all --sequential adminbuildwp tailwindbuild",
    "adminstart": "npm-run-all --parallel adminwpstart tailwindwatch",
    "adminbuildwp": "wp-scripts build src/admin-page/index.js --output-path=build/admin-page",
    "adminwpstart": "wp-scripts start src/admin-page/index.js --output-path=build/admin-page",
    "admintailwindbuild": "tailwindcss -i ./src/admin-page/index.css -o ./build/admin-page/index.css --postcss --minify",
    "admintailwindwatch": "tailwindcss -i ./src/admin-page/index.css -o ./build/admin-page/index.css --watch --postcss --minify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emoji-mart/data": "^1.1.2",
    "@emoji-mart/react": "^1.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@wordpress/element": "^5.29.0",
    "@wordpress/i18n": "^4.52.0",
    "@wordpress/scripts": "^27.3.0",
    "autoprefixer": "^10.4.17",
    "axios": "^1.6.7",
    "browser-sync": "^3.0.2",
    "emoji-mart": "^5.5.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "postcss-nested": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.1",
    "tailwindcss": "^3.4.1",
    "use-immer": "^0.9.0"
  }
}
